{

    "manifest_version": 2,
    "name": "Steam Randomize Profile",
    "version": "1.0",
  
    "description": "Extension to make it possible to randomly select certain items in your Steam profile",
    "homepage_url": "https://github.com/MrGameandCode/SteamRandomProfile",
    "icons": {
      "48": "icons/Steam-Random-Logo.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.steamcommunity.com/profiles/*"],
        "js": ["SteamRandom.js"]
      }
    ]
  
  }